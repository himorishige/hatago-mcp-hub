{
  "$schema": "https://raw.githubusercontent.com/himorishige/hatago-mcp-hub/main/schemas/config.schema.json",
  "version": 1,
  "timeouts": {
    "requestMs": 120000,
    "keepAliveMs": 15000
  },
  "mcpServers": {
    "deepwiki": {
      "url": "https://mcp.deepwiki.com/mcp",
      "timeouts": {
        "requestMs": 60000
      },
      "tags": ["dev"]
    },
    "cloudflare-docs": {
      "url": "https://docs.mcp.cloudflare.com/sse",
      "type": "sse",
      "tags": ["dev"],
      "timeouts": {
        "requestMs": 300000,
        "connectMs": 15000,
        "keepAliveMs": 20000
      }
    },
    "cloudflare-bindings": {
      "url": "https://bindings.mcp.cloudflare.com/sse",
      "type": "sse",
      "tags": ["dev"],
      "timeouts": {
        "requestMs": 300000,
        "connectMs": 15000,
        "keepAliveMs": 20000
      }
    },
    "cloudflare-builds": {
      "url": "https://builds.mcp.cloudflare.com/sse",
      "type": "sse",
      "tags": ["dev"],
      "timeouts": {
        "requestMs": 300000,
        "connectMs": 15000,
        "keepAliveMs": 20000
      }
    },
    "byterover-mcp": {
      "url": "https://mcp.byterover.dev/v2/mcp",
      "headers": {
        "Authorization": "Bearer ${BYTE_ROVER_API_TOKEN}"
      },
      "timeouts": {
        "requestMs": 240000,
        "connectMs": 15000
      },
      "tags": ["dev"]
    }
  }
}
