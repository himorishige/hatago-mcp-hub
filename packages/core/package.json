{
  "name": "@himorishige/hatago-core",
  "version": "0.0.3",
  "description": "Core types and protocol definitions for Hatago MCP Hub",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "import": "./dist/schemas.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "format": "prettier --write ./src",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "tsdown": "^0.14.2",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/himorishige/hatago-mcp-hub.git"
  },
  "bugs": {
    "url": "https://github.com/himorishige/hatago-mcp-hub/issues"
  },
  "homepage": "https://github.com/himorishige/hatago-mcp-hub#readme"
}
