{
  "version": 1,
  "logLevel": "debug",
  "http": {
    "port": 3001,
    "host": "localhost"
  },
  "mcpServers": {
    "filesystem": {
      "type": "local",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/tmp"
      ],
      "disabled": false,
      "timeouts": {
        "connectMs": 3000,
        "requestMs": 15000,
        "keepAliveMs": 10000
      }
    },
    "disabled-server": {
      "type": "local",
      "command": "echo",
      "args": ["This should not run"],
      "disabled": true
    }
  }
}