{
  "version": 1,
  "logLevel": "info",
  "mcpServers": {
    "slow-server": {
      "url": "https://example.com/mcp",
      "type": "sse",
      "hatagoOptions": {
        "start": "lazy",
        "timeouts": {
          "timeout": 60000,
          "maxTotalTimeout": 300000,
          "resetTimeoutOnProgress": true
        }
      }
    },
    "fast-server": {
      "url": "https://fast.example.com/mcp",
      "type": "http",
      "hatagoOptions": {
        "start": "eager",
        "timeouts": {
          "timeout": 10000,
          "maxTotalTimeout": 60000,
          "resetTimeoutOnProgress": false
        }
      }
    },
    "default-timeout-server": {
      "url": "https://default.example.com/mcp",
      "type": "http",
      "hatagoOptions": {
        "start": "lazy"
      }
    },
    "local-server": {
      "command": "node",
      "args": ["./server.js"],
      "hatagoOptions": {
        "start": "eager",
        "tools": {
          "prefix": "local"
        }
      }
    }
  }
}