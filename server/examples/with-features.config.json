{
  "profile": "default",
  "features": {
    "healthCheck": true,
    "metrics": false,
    "tracing": false,
    "authentication": false,
    "rateLimit": false,
    "typeGeneration": false,
    "openapi": false
  },
  "servers": [
    {
      "id": "python-server",
      "type": "local",
      "command": "python",
      "args": ["./mcp_server.py"],
      "description": "Python MCP server"
    },
    {
      "id": "nodejs-server",
      "type": "local",
      "command": "node",
      "args": ["./server.js"],
      "env": {
        "NODE_ENV": "production"
      }
    }
  ],
  "generation": {
    "autoReload": true,
    "watchPaths": ["./examples/with-features.config.json"]
  }
}