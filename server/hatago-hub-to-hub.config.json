{
  "version": 1,
  "logLevel": "debug",
  "mcpServers": {
    "filesystem-local": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "env": {},
      "hatagoOptions": {
        "start": "eager"
      }
    },
    "remote-hatago": {
      "type": "remote",
      "url": "http://localhost:3001/mcp/v1/sse",
      "transport": "streamable-http",
      "hatagoOptions": {
        "start": "eager",
        "timeout": 30000
      }
    }
  }
}