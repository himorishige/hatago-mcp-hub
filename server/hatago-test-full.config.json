{
  "profile": "test-full",
  "description": "Hatago test configuration with remote, npx, and local servers",
  "features": {
    "healthCheck": false,
    "metrics": false,
    "tracing": false,
    "authentication": false,
    "rateLimit": false,
    "typeGeneration": false,
    "openapi": false
  },
  "servers": [
    {
      "id": "everything",
      "type": "npx",
      "package": "@modelcontextprotocol/server-everything",
      "description": "Everything MCP server via npx",
      "enabled": true
    },
    {
      "id": "filesystem",
      "type": "local",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "description": "Filesystem MCP server for /tmp",
      "enabled": true
    },
    {
      "id": "test-node",
      "type": "local",
      "command": "node",
      "args": ["examples/test-mcp-server.js"],
      "cwd": "/Users/morishige/ghq/github.com/himorishige/hatago-hub/server",
      "description": "Test Node.js MCP server",
      "enabled": true
    },
    {
      "id": "deepwiki",
      "type": "remote",
      "url": "https://mcp.deepwiki.com/sse",
      "transport": "sse",
      "description": "DeepWiki MCP server",
      "enabled": true
    }


  ],
  "http": {
    "port": 3000,
    "host": "localhost"
  },
  "generation": {
    "autoReload": false
  }
}